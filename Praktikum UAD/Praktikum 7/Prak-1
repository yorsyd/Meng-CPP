#include <iostream>
using namespace std;

int main(){
// Langkah Praktikum No.2
  	int i,j, n=5;
  	for (i=1;i<=n;i++){
  		for (j=1; j<=i;j++){
  			cout << 1;
  		}
  		cout << endl;
  	}

// Langkah Praktikum No.4 
	int i,j, n=5;
	for (i=1;i<=n;i++){
		for (j=1; j<=i;j++){
			cout << j;
		}
		cout << endl;
	}
}
